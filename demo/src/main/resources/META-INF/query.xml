<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmls="http://xmlns.jcp.org/xml/hs/ns/persistence/orm" version="2.1">
    <name-query name="Member.findByUsername">
        <query><CDATA [
            SELECT m
            FROM Member m
            WHERE m.name = :name
            ]></query>
    </name-query>
    <name-query name="Member.findByUsername">
        <query>SELECT COUNT(m) FROM Member m</query>
    </name-query>
</entity-mappings>